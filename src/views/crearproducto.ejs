<%- include("template/header.ejs") %>
  <section class="cre">
    <h2>Crear Producto</h2>
  </section>
  <section class="crearprod">
    <form action="/productos" method="POST">
      <input type="text" placeholder="Digite Producto" name="nombre">
      <input type="text" placeholder="Descripcion del producto" name="descripcion">
      <input type="number" placeholder="Digite cantidad" name="cantidad">
      <input type="number" placeholder="Digite precio sin iva" name="preciosin">
      <input type="number" placeholder="Digite iva" name="iva">
      <input type="text" placeholder="Digite porveedor" name="proveedor">
      <button type="submit">Guardar</button>


    </form>
  </section>

  </body>

  </html>